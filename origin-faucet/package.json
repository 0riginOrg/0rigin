{
  "name": "origin-faucet",
  "description": "Origin TestNet faucet",
  "version": "0.1.0",
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OriginProtocol/origin/origin-faucet"
  },
  "bugs": {
    "url": "https://github.com/OriginProtocol/origin/issues"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint . && npm run prettier:check",
    "prettier": "prettier --write *.js \"src/**/*.js\"",
    "prettier:check": "prettier -c *.js \"src/**/*.js\"",
    "start": "per-env",
    "start:development": "nodemon faucet/app.js",
    "start:production": "node faucet/app.js"
  },
  "author": "Origin Protocol Inc",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/register": "^7.0.0",
    "bignumber.js": "^8.0.1",
    "dotenv": "^6.2.0",
    "envkey": "^1.2.4",
    "express": "^4.16.4",
    "origin-contracts": "^0.8.6",
    "origin-token": "^0.1.0",
    "per-env": "^1.0.2",
    "rate-limiter-flexible": "^0.19.4",
    "truffle-hdwallet-provider": "0.0.6",
    "truffle-privatekey-provider": "^0.1.0",
    "web3": "1.0.0-beta.34"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
