{
  "$id":"unit-listing.json",
  "title": "UnitListing",
  "$merge": {
    "source": { "$ref": "listing.json" },
    "with": {
      "required": ["name", "priceWei"],
      "properties": {
        "priceWei": {
          "type": "number"
        },
        "unitsAvailable": {
          "type": "number"
        }
      }
    }
  }
}
