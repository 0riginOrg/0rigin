{
	"name": "@origin/dapp-creator-client",
	"version": "1.0.0",
	"description": "Tool of creating configuration files for Origin DApps",
	"main": "src/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/OriginProtocol/origin"
	},
	"keywords": [
		"Origin Protocol",
		"DApp",
		"Ethereum"
	],
	"author": "Origin Protocol Inc",
	"license": "MIT",
	"dependencies": {
		"clean-webpack-plugin": "^2.0.1",
		"webpack-cli": "3.3.0"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		],
		"plugins": [
			[
				"module-resolver",
				{
					"alias": {
						"actions": "./src/actions",
						"components": "./src/components",
						"pages": "./src/pages",
						"reducers": "./src/reducers",
						"utils": "./src/utils"
					}
				}
			],
			[
				"@babel/plugin-proposal-decorators",
				{
					"legacy": true
				}
			]
		]
	},
	"prettier": {
		"semi": false,
		"singleQuote": true
	}
}
