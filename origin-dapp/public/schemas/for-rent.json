{
  "$schema":"http://json-schema.org/draft-06/schema#",
  "type": "object",
  "required": ["name","category","description"],
  "properties": {
    "listingType": {
      "const": "fractional"
    },
    "slotIncrementMinutes": {
      "type": "number",
      "default": 1440
    },
    "name": {
      "type": "string",
      "title": "schema.forRent.title",
      "minLength": 3,
      "maxLength": 100
    },
    "examples": {
      "type": "string",
      "title": "schema.forRent.examples",
      "enum": [
        "schema.forRent.bicycles",
        "schema.forRent.carsTrucks",
        "schema.forRent.electronics",
        "schema.forRent.housing"
      ]
    },
    "category": {
      "type": "string",
      "title": "schema.forRent.category",
      "enum": [
        "schema.forSale.appliances",
        "schema.forSale.atvsUtvsSnowmobiles",
        "schema.forSale.babyKidStuff",
        "schema.forSale.bicycles",
        "schema.forSale.boats",
        "schema.forSale.carsTrucks",
        "schema.forSale.cellPhones",
        "schema.forSale.clothingAccessories",
        "schema.forSale.computers",
        "schema.forSale.electronics",
        "schema.forSale.farmGarden",
        "schema.forSale.furniture",
        "schema.forSale.healthBeauty",
        "schema.forSale.heavyEquipment",
        "schema.forSale.householdItems",
        "schema.forSale.housing",
        "schema.forSale.jewelry",
        "schema.forSale.motorcyclesScooters",
        "schema.forSale.musicalInstruments",
        "schema.forSale.other",
        "schema.forSale.parking",
        "schema.forSale.recreationalVehicles",
        "schema.forSale.sportingGoods",
        "schema.forSale.storage",
        "schema.forSale.tools",
        "schema.forSale.toysGames",
        "schema.forSale.trailers",
        "schema.forSale.videoGaming"
      ],
      "default":"schema.forRent.housing"
    },
    "description": {
      "type": "string",
      "title": "schema.forRent.description",
      "minLength": 10,
      "maxLength": 1024
    },
    "price": {
      "type": "number",
      "title": "schema.forRent.priceInETH"
    },
    "pictures": {
      "type": "array",
      "title": "schema.forRent.selectPhotos",
      "items": {
        "type": "string",
        "format": "data-url"
      }
    },
    "sellerSteps": {
      "type": "string",
      "title": "schema.forRent.sellerSteps",
      "enum": [
        "schema.forRent.messageBuyer",
        "schema.forRent.prepareProperty",
        "schema.forRent.rateBuyer"
      ]
    }
  }
}
