      "BRIDGE_SERVER_PATH": "/opt/origin-bridge/source",
      "BRIDGE_SERVER_ENV_PATH": "/opt/origin-bridge/origin-bridge-venv"
    }
  },{
    "name"       : "postgresql",
    "script"     : "start-pg.sh",
    "cwd"        : "/opt/pg",
    "interpreter": "/bin/bash"
  },{
    "name"       : "elasticsearch",
    "script"     : "start-es.sh",
    "cwd"        : "/opt/es",
    "interpreter": "/bin/bash"
  },{
    "name"       : "js",
    "script"     : "start-js.sh",
    "cwd"        : "/opt/origin-js/scripts",
